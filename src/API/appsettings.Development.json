{
  "PasswordSettings": {
    "RequireDigit": false,
    "RequiredLength": 8,
    "RequireNonAlphanumeric": false,
    "RequireUppercase": false,
    "RequireLowercase": false
  },
  "CorsSettings": {
    "AllowedHosts": "*"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
